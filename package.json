    "migration:run": "ts-node migration run",
    "migration:revert": "ts-node migration revert"
  },
  "dependencies": {
    "@nestjs/axios": "^0.1.0",
    "@vendure/admin-ui-plugin": "1.7.3",
    "@vendure/asset-server-plugin": "1.7.3",
    "@vendure/core": "1.7.3",
    "@vendure/email-plugin": "1.7.3",
    "dotenv": "16.0.2",
    "pg": "8.8.0",
    "typescript": "4.3.5"
  },
  "devDependencies": {
    "concurrently": "7.4.0",
    "ts-node": "10.9.1"
  }
}
